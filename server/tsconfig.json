{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",

    "outDir": "./dist",
    "rootDir": "./",
    "removeComments": true,

    "strict": true,

    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node",
      "socket.io"
    ]
  }
}
